    <Group>
      <Name>ㅤㅤㅍㅎ</Name>
      <Enabled>True</Enabled>
    </Group>
    <Group>
      <Name>후처리</Name>
      <Enabled>True</Enabled>
    </Group>
    <Group>
      <Name>ㅤ공백제거</Name>
      <Enabled>True</Enabled>
      <MultipleSearchAndReplaceItem>
        <Enabled>True</Enabled>
        <FindWhat>^\s$</FindWhat>
        <ReplaceWith />
        <SearchType>RegularExpression</SearchType>
        <Description>공백만 존재하는 문장 제거</Description>
      </MultipleSearchAndReplaceItem>
    </Group>
  </MultipleSearchAndReplaceList>
</Settings>