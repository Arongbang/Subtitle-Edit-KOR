      </MultipleSearchAndReplaceItem>
      <MultipleSearchAndReplaceItem>
        <Enabled>True</Enabled>
        <FindWhat>([\w]{2})왔다([\.\?])?(?:[^고])</FindWhat>
        <ReplaceWith>$1왔어$2</ReplaceWith>
        <SearchType>RegularExpression</SearchType>
        <Description>다-왔다</Description>
      </MultipleSearchAndReplaceItem>
      <MultipleSearchAndReplaceItem>
        <Enabled>True</Enabled>
        <FindWhat>([\w]{1})지\s않다([\.\?])?(?:[^고])</FindWhat>
        <ReplaceWith>안 $1아$2</ReplaceWith>
        <SearchType>RegularExpression</SearchType>
        <Description>다-지 않다</Description>
      </MultipleSearchAndReplaceItem>
      <MultipleSearchAndReplaceItem>
        <Enabled>True</Enabled>
        <FindWhat>([\w]{1})했다([\.\?])?(?:[^고])</FindWhat>
        <ReplaceWith>$1했어$2</ReplaceWith>
        <SearchType>RegularExpression</SearchType>
        <Description>다-했다</Description>
      </MultipleSearchAndReplaceItem>
      <MultipleSearchAndReplaceItem>
        <Enabled>True</Enabled>
        <FindWhat>(?:이)?(봐)요([\,\.\!])</FindWhat>
        <ReplaceWith>$1$1$2</ReplaceWith>
        <SearchType>RegularExpression</SearchType>
        <Description>요-봐요</Description>
      </MultipleSearchAndReplaceItem>
      <MultipleSearchAndReplaceItem>
        <Enabled>True</Enabled>
        <FindWhat>([\w]+)어요([\.\?]+)</FindWhat>
        <ReplaceWith>$1어$2</ReplaceWith>
        <SearchType>RegularExpression</SearchType>
        <Description>요-어요</Description>
      </MultipleSearchAndReplaceItem>
      <MultipleSearchAndReplaceItem>
        <Enabled>True</Enabled>
        <FindWhat>(잠깐만)요([\.\?])?</FindWhat>
        <ReplaceWith>$1$2</ReplaceWith>
        <SearchType>RegularExpression</SearchType>
        <Description>요-잠깐만요</Description>
      </MultipleSearchAndReplaceItem>
      <MultipleSearchAndReplaceItem>
        <Enabled>True</Enabled>
        <FindWhat>(?:큰가요|큰\s거에요)(\?)</FindWhat>
        <ReplaceWith>커진거야$1</ReplaceWith>
        <SearchType>RegularExpression</SearchType>
        <Description>요-커진거야</Description>
      </MultipleSearchAndReplaceItem>
      <MultipleSearchAndReplaceItem>
        <Enabled>True</Enabled>
        <FindWhat>([\w]+)하고\s있나요([\.\?])?</FindWhat>
        <ReplaceWith>$1$2</ReplaceWith>
        <SearchType>RegularExpression</SearchType>
        <Description>줄인말-하고 있나요</Description>
      </MultipleSearchAndReplaceItem>
      <MultipleSearchAndReplaceItem>
        <Enabled>True</Enabled>
        <FindWhat>([\w]+)이\s아니지\s않습니까([\.\?])?</FindWhat>
        <ReplaceWith>$1은 아니지$2</ReplaceWith>
        <SearchType>RegularExpression</SearchType>
